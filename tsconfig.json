{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "declaration": false,
    "noEmitHelpers": true,
    "experimentalDecorators": true,
    "outDir": "dist",
    "lib": [
        "es2016"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}